{
  "name": "@useoptic/openapi-utilities",
  "packageManager": "yarn@3.0.2",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "dev:test": "yarn tap --no-timeout --ts src",
    "dev:test-file": "yarn tap --ts --no-timeout $FILE",
    "dev:test-watch": "yarn tap --ts src --watch",
    "ci:test": "yarn tsc && yarn tap build",
    "build": "yarn tsc --build --verbose && copyfiles -u 1 src/**/*.js build/"
  },
  "devDependencies": {
    "@types/copyfiles": "^2",
    "@types/fs-extra": "^9.0.13",
    "@types/tap": "^15.0.5",
    "prettier": "^2.4.1",
    "tap": "^15.0.10",
    "ts-node": "^10.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.9",
    "@jsdevtools/ono": "^7.1.3",
    "commander": "^8.3.0",
    "copyfiles": "^2.4.1",
    "fast-deep-equal": "^3.1.3",
    "fs-extra": "^10.0.0",
    "json-ptr": "^2.2.0",
    "openapi-types": "^9.3.0",
    "swagger-client": "^3.17.0",
    "ts-invariant": "^0.9.3",
    "yaml-ast-parser": "^0.0.43"
  }
}
