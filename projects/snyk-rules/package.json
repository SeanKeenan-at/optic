{
  "name": "snyk-api-checks",
  "packageManager": "yarn@3.0.2",
  "version": "0.1.0",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "dev:test": "yarn tap --no-timeout --ts src/**/*.test.ts",
    "build": "yarn tsc --build --verbose && copyfiles -u 1 src/**/*.js build/"
